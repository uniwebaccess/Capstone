{
  "name": "UniWebAccess",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "firebase": "^8.2.5",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "path": "^0.12.7",
    "puppeteer": "^5.5.0",
    "readline": "^1.3.0",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "server-dev": "nodemon backend/server.js",
    "client-dev": "cd client && npm run start",
